package OCT26;

public class CountRepetativeDigits {
	
	public static void main (String args[]) {
		
		int[] a = {1,1,2,2,3,4,5,5,6,6};
		int count = 0;
		int k = 0;
		for(int i =0; i<a.length; i++) {
			
			for(int j = 0; j<a.length;j++) {
				
				if(a[i] == a[j])
					count = count + 1;
				k = a[j];
				
			}
			System.out.println(k + ": "+ count);
			count = 0;
			k = 0;
		}
	}

}
