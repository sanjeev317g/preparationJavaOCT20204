package practice;

public class primeNumbers {
	
	public static void main(String args[]) {
		int number = 100;
		System.out.println("Prime numbers between 2 - 100 :");
		for(int i=2;i<number;i++) {
			if(i%2 == 0) {
				//System.out.println("Not a Prime Number ");
			}
			else {
				
				System.out.print(i+",");
			}
		}
	}

}
