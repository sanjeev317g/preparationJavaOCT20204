package practice;

public class SearchElementInArray {
	
	public static void main(String args[]) {
		
		int[] a = {1,2,3,4,5,6,8,99};
		
		int search = 9;
		
		for(int i=0;i<a.length;i++) {
			
			if(a[i] == search) {
				System.out.println("Index of: "+ i);
			}
			else {
				System.out.println("Not in Index");
			}
		}
	}

}
