package OCT26;

public class CountRepetativeDigits {
	
	public static void main (String args[]) {
		
		int[] a = {1,1,2,2,3,4,5,5,6,6};
		int count = 0;
		int j;
		for(int i =0; i<a.length; i++) {
			
			for(j=1 ; j<a.length;j++) {
				
				if(a[i] == a[j])
					count = count + 1;
			}
			System.out.println(a[j] + ": "+ count);
			count = 0;
			
		}
	}

}
