package OCT26;

public class Anagram {
	
	public static void main(String args[]) {
		
		String str1 = "Listen";
		String str2 = "Silente";
		
		if(str1.toLowerCase().length() == str2.toLowerCase().length()) {
			System.out.println("Anagram first condition satisfied");
		}
		else {
			System.out.println("Not an Anagram");
		}
		
		
	}

}
