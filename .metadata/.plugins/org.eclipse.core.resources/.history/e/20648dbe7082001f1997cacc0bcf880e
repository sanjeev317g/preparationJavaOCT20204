package practice;

public class OutInPut {
	
	public static void main(String args[]) {
		
		String input = "32400121200";
		
		int zeroCount = 0;
		
		StringBuffer bf = new StringBuffer();
		
		for(char c: input.toCharArray()) {
			if(c == '0') {
				zeroCount = zeroCount +1;
				bf.append(c);
			}
		}
		
		

	}

}
