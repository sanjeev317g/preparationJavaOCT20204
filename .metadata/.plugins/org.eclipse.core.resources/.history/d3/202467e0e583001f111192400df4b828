package practice;

public class MinimumAndMaxiumArray {
	
	public static void main(String args[]) {
		
		int[] a = {1,2,3,4,5,6,7,8,9};
		
		int min = 0;
		
		int max = 0;
		
		for(int i=1;i<a.length;i++) {
			
			for(int j=0;j<a.length;i++) {
				
				int aa = a[i];
				
				int bb = a[j];
				
				if(a[i] < a[j]) {
					min = a[j];
				}
				if(a[i] > a[j]) {
					max = a[i];
				}
			}
		}
		System.out.println(min + " "+ max);
	}

}
