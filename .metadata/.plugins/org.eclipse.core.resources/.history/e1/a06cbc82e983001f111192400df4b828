package practice;

public class SearchElementInArray {
	
	public static void main(String args[]) {
		

	}
	
	public static boolean search() {
		
		int[] a = {1,2,3,4,5,6,8,99};
		
		int search = 99;
		
		for(int i=0;i<a.length;i++) {
			
			if(a[i] == search) {
				
				return true;
			}

		}
		return false;
		
		
	}

}
